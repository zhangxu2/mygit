include $(SWHOME)/make/sw.env

USERLIB=-lswbase -luni -ldl
TARGET=uni_dbconfig_test
OBJS=uni_dbconfig_test.o

ALL:$(TARGET)
$(TARGET):$(OBJS)
	$(CC) $(LFLAGS)  -o  $@ $(OBJS) $(APPLIBDIR) $(USERLIB)
	@rm -f *.o
	@echo "$@ is OK !"

.SUFFIXES: .c .o
.c.o:
	$(CC) -v $(CFLAGS)  $(APPINCLUDE) -I$(SWHOME)/src/uniform -c $*.c

clean:
	rm -f *.o $(TARGET)

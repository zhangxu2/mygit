include $(SWHOME)/make/sw.env
USERLIB = -lswbase -losbase
SLIB = test_run

OBJS = run_test.o

all: $(SLIB) clean

$(SLIB): $(OBJS)
	$(CC) $(LFLAGS) $(APPLIBDIR) $(USERLIB) -o $@ $(OBJS)
	@echo "$@ is OK !"
.SUFFIXES: .c .o
.c.o:
	$(CC)  $(CFLAGS) $(APPINCLUDE) -c $*.c

clean:
	@rm -f *.o core trc.log
